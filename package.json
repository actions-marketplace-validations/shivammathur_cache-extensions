{
  "name": "cache-extensions",
  "version": "1.5.1",
  "private": false,
  "description": "Cache extensions in GitHub Actions",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint **/*.ts --cache",
    "format": "prettier --write **/*.ts && git add .",
    "format-check": "prettier --check **/*.ts",
    "release": "ncc build src/cache.ts -o dist && git add -f dist/",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shivammathur/cache-extensions.git"
  },
  "keywords": [
    "actions",
    "php",
    "setup"
  ],
  "author": "shivammathur",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run format && npm run lint && npm run test && npm run build && npm run release"
    }
  }
}
